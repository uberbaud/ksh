#: FUNCTION ^S…^s
#:   Wrapper

function f-luarocks {
	local pgm
	[[ -e /usr/local/bin/luarocks-5.1 ]]&& pgm=/usr/local/bin/luarocks-5.1
	[[ -e /usr/local/bin/luarocks ]]&& pgm=/usr/local/bin/luarocks
	"$pgm" "$@"
}

