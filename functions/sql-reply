#: FUNCTION ^G(no arguments)^g
#:   Prints ^Ssqlreply^s tab-delimited.

needs sql-fields
function sql-reply {
	local T
	for T in "${sqlreply[@]}"; do
		sql-fields "$T"
	done 
}

