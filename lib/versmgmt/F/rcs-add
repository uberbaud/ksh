#: FUNCTION ^Ufilename^u ^Udescription^u
#:   vmgr vms-cmd

needs ci
function rcs-add {
	local file
	file=${1:?}; shift
	[[ -f RCS/$file,v ]]||
		ci -q -i -t-"$*" -u ./"$file" 2>&1
}

