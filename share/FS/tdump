#: FUNCTION ^Ufile^u
#:   ^Thexdump -C^t ^Ufile^u ^T| less^t (like Borland's Turbo Dump)

needs hexdump
function tdump {
	(($#==1))|| die "Expected one argument: the ^Ufile^u name."
	[[ -f $1 ]]|| die "^B$1^b is not a file."
	hexdump -C -- "$1" | less
}

