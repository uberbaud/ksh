#: FUNCTION ^G(no arguments)^g
#:   vmgr vms-cmd

function got-status {
	local file
	file=${1:?}
	set -- $(got status "$file")
	(($#))|| {
		if [[ -n $(got status -I "$file") ]]; then
			STATUS=ignored
		else
			STATUS=ok
		fi
		return 0
	  }
	case ${1:?WHAT\?} in
		\?) STATUS=untracked;	;;
		M)	STATUS=modified;	;;
		A)	STATUS=ok;			;;
		D)	STATUS=ok;			;;
		*)	STATUS=error;		;;
	esac
}

