# vim: filetype=ksh tabstop=4 textwidth=72 noexpandtab

#: FUNCTION ^G(no arguments)^g
#:   Wrapper around prj.ksh for ^Tcd^t.

function f-prj {
	local P
	: ${BS:?}
	if [[ $1 == @(help|inc|ls|new) ]]; then
		$BS/prj.ksh "$@"
	else
		P=$($BS/prj.ksh "$@")
		[[ -n $P ]]&& { cd ~/projects/"$P"; ls; }
	fi
}

