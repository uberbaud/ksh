#: FUNCTION ^G(no arguments)^g
#:   Creates a function ^SisOldFunction^s for ^Tfchanged^t to test against.

function fmark {
	set -- $(typeset +f)
	local IFS='|'
	eval "function isOldFunction { [[ \${1:?} == @($*) ]]; }"
}

