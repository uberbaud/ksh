#: FUNCTION
#:   vmgr vms-cmd

needs rcs-changed
function rcs-status {
	STATUS=untracked
	[[ -f RCS/$1,v ]]&&
		if rcs-changed "$1"; then
			STATUS=modified
		else
			STATUS=ok
		fi
}

