#: FUNCTION ^G(no arguments)^g
#:   A wrapper around all m-*

needs matching-commands
function m {
	(($#))|| { matching-commands m-\*; return; }
	[[ $1 == +([0-9]) ]]&& set -- part "$@"
	m-"$@"
}

