#: FUNCTION ^G(same arguments/^SSTDIN^s behavior as ^Bcowsay^b)^g
#:   ^Tcowsay^t through ^Tlolcat^t

function lolcow {
	needs cowsay lolcat
	cowsay "$@" | lolcat
}


